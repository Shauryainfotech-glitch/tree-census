# TMC Tree Census App Environment Variables

# API Configuration
VITE_API_BASE_URL=https://api.thanecity.gov.in/tree-census
VITE_AI_SERVICE_URL=https://ai.thanecity.gov.in/v1

# AI Service Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_AI_CONFIDENCE_THRESHOLD=0.85
VITE_AUTO_APPROVE_THRESHOLD=0.95

# Map Configuration
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_DEFAULT_LAT=19.2183
VITE_DEFAULT_LNG=72.9781

# Application Configuration
VITE_APP_NAME=TMC Tree Census
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# Storage Configuration
VITE_OFFLINE_STORAGE_LIMIT=50
VITE_CACHE_TTL_MINUTES=60
VITE_AUTO_SYNC_INTERVAL_MINUTES=5

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=true

# Contact Information
VITE_SUPPORT_EMAIL=treeauthority@thanecity.gov.in
VITE_SUPPORT_PHONE=+91 22 2536 2000
VITE_ORGANIZATION_NAME=Thane Municipal Corporation

# Security
VITE_SESSION_TIMEOUT_MINUTES=30
VITE_MAX_FILE_SIZE_MB=10
VITE_ALLOWED_FILE_TYPES=jpg,jpeg,png,pdf

# Notification Settings
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_NOTIFICATION_VAPID_KEY=your_vapid_key_here
